{
  "name": "animation-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start --dev-client",
    "start:2": "expo start --port 9090",
    "android": "expo run:android",
    "android:2": "react-native run-android --port 8081 --deviceId emulator-5556",
    "android:3": "react-native run-android --port 8081 --deviceId emulator-5558",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "reset": "rm -rf node_modules/ && npm i",
    "clean:android": "cd android && ./gradlew cleanBuildCache && ./gradlew clean && cd ..",
    "clean:ios": "cd ios/ && rm -rf Pods/ build/ Podfile.lock && cd ..",
    "reset:ios": "rm -rf node_modules/ && npm run clean:ios",
    "reset:android": "rm -rf node_modules/ && cd android && rm -rf /app/build build/ .gradle/",
    "delete-lock": "rm  ~/.gradle/caches/7.5.1/fileHashes/fileHashes.lock && cd android && ./gradlew --stop && cd .. "
  },
  "dependencies": {
    "@react-navigation/drawer": "^6.5.0",
    "@react-navigation/native": "^6.0.13",
    "expo": "~47.0.6",
    "expo-splash-screen": "~0.17.5",
    "expo-status-bar": "~1.4.2",
    "react": "18.1.0",
    "react-native": "0.70.5",
    "react-native-gesture-handler": "~2.8.0",
    "react-native-reanimated": "~2.12.0",
    "react-native-safe-area-context": "4.4.1",
    "react-native-screens": "~3.18.0",
    "react-native-svg": "13.4.0",
    "react-native-webrtc": "1.106.1",
    "socket.io-client": "^4.5.3",
    "expo-device": "~5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.3",
    "@types/react": "^18.0.25",
    "@types/react-native": "^0.70.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "react-native-svg-transformer": "^1.0.0",
    "typescript": "^4.9.3"
  },
  "private": true
}
